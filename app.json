{
  "expo": {
    "name": "PiDevKit IDE",
    "slug": "pidevkit-repl",
    "version": "1.0.0",
    "scheme": "pidevkit-repl",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121E2E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pidevkit.ide"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121E2E"
      },
      "package": "com.pidevkit.ide"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb91563e-7849-4360-9653-faf7fb10f3d6"
      }
    }
  }
}
